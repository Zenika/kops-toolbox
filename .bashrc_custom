source <(kubectl completion bash)
source <(kops completion bash)
complete -C ~guest/.local/bin/aws_completer aws
if grep $KOPS_USER .aws/credentials >/dev/null; then
  . ~/bin/source-kops-env.sh
  CURRENT_KOPS_USER=$(aws iam get-user --output=text | awk '{ print $6 }')
  if [ "$CURRENT_KOPS_USER" == "$KOPS_USER" ]; then
    echo "Kops user initialized as $KOPS_USER"
  else
    echo "Error, current user $CURRENT_KOPS_USER is not kops user defined as $KOPS_USER"
  fi
else
  echo "Please create kops user then source it"
fi
